{
    "name": "@gridsuite/commons-ui",
    "version": "0.49.0",
    "description": "common react components for gridsuite applications",
    "engines": {
        "npm": ">=9",
        "node": ">=18"
    },
    "exports": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "sideEffects": [
        "**/*.css"
    ],
    "scripts": {
        "start": "vite demo/ --config vite.config.mts",
        "start:open": "vite demo/ --config vite.config.mts --open",
        "build": "tsc && vite build",
        "build:prune": "npm install && tsc && vite build && npm prune --production",
        "prepublishOnly": "npm run build",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "test:watch": "jest --watch",
        "lint": "eslint . --ext js,mjs,jsx,ts,mts,tsx --max-warnings 0"
    },
    "dependencies": {
        "autosuggest-highlight": "^3.2.0",
        "clsx": "^1.0.4",
        "jwt-decode": "^3.0.0",
        "memoize-one": "^6.0.0",
        "oidc-client": "^1.10.1",
        "prop-types": "^15.7.2",
        "react-csv-downloader": "^2.7.0",
        "react-request-fullscreen": "^1.1.2",
        "react-virtualized": "^9.22.5",
        "uuid": "^3.4.0"
    },
    "peerDependencies": {
        "@emotion/react": "^11.8.1",
        "@emotion/styled": "^11.8.1",
        "@mui/icons-material": "^5.5.0",
        "@mui/lab": "^5.0.0-alpha.72",
        "@mui/material": "^5.5.0",
        "@hookform/resolvers": "^3.0.0",
        "notistack": "^3.0.0",
        "react-router-dom": "^6.0.0",
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
        "react-intl": "^6.0.0",
        "react-hook-form": "^7.41.0",
        "yup": "^1.0.0"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.23.8",
        "@babel/preset-react": "^7.23.3",
        "@babel/preset-typescript": "^7.23.3",
        "@emotion/react": "^11.8.1",
        "@emotion/styled": "^11.8.1",
        "@mui/icons-material": "^5.5.0",
        "@mui/lab": "^5.0.0-alpha.72",
        "@mui/material": "^5.5.0",
        "@mui/styles": "^5.5.0",
        "@hookform/resolvers": "^3.0.0",
        "@babel/helper-builder-react-jsx": "7.15.4",
        "@types/react": "^18.2.56",
        "@types/react-dom": "^18.2.19",
        "@typescript-eslint/eslint-plugin": "5.62.0",
        "@typescript-eslint/parser": "5.62.0",
        "@vitejs/plugin-react": "^4.2.1",
        "babel-eslint": "^10.1.0",
        "bufferutil": "^4.0.1",
        "eslint": "^7.9.0",
        "eslint-config-prettier": "^8.0.0",
        "eslint-config-react-app": "^6.0.0",
        "eslint-plugin-flowtype": "^5.2.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jsx-a11y": "^6.3.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.21.0",
        "eslint-plugin-react-hooks": "^4.1.2",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "jest-svg-transformer": "^1.0.0",
        "notistack": "^3.0.0",
        "prettier": "^2.0.0",
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
        "react-intl": "^6.0.0",
        "react-hook-form": "^7.41.0",
        "react-router-dom": "^6.0.0",
        "ts-node": "^10.9.2",
        "type-fest": "^2.0.0",
        "typescript": "5.1.6",
        "utf-8-validate": "^5.0.2",
        "@react-hook/window-size": "^3.1.1",
        "react-resizable": "^3.0.4",
        "vite": "^5.0.11",
        "vite-plugin-eslint": "^1.8.1",
        "vite-plugin-externalize-deps": "^0.8.0",
        "vite-plugin-svgr": "^4.2.0",
        "vite-plugin-dts": "^2.3.0",
        "vite-plugin-lib-inject-css": "^2.0.0",
        "yup": "^1.0.0"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "plugin:prettier/recommended"
        ],
        "ignorePatterns": [
            "dist"
        ],
        "rules": {
            "prettier/prettier": "warn",
            "curly": "error"
        }
    },
    "author": "gridsuite team",
    "homepage": "https://github.com/gridsuite",
    "license": "MPL-2.0",
    "repository": "git://github.com/gridsuite/commons-ui.git"
}
